{{#if submitlist}}
<ol class="breadcrumb col-md-11 margin-20">
  <li><a href="#user">{{user}}</a></li>
  <li><a href="#courseinfo?cid={{cid}}">{{course_name}}</a></li>
  <li><a href="#examresult?cid={{cid}}&eid={{eid}}">{{examname}}</a></li>
  <li class="active">提交列表</li>
</ol>
<table class="table table-striped table-hover line-btn">
  <thead>
    <tr>
      <th>学号</th>
      <th>姓名</th>
      <th>交卷时间</th>
      <th>分数</th>
    </tr>
  </thead>
  <tbody>
    {{#each submitlist}}
    <tr onclick="document.location='#studentanswer?cid={{../cid}}&&eid={{../eid}}&student={{studentid}}'">
      <td>{{studentid}}</td>
      <td>{{name}}</td>
      <td>{{time}}</td>
      <td>{{score}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
{{else}}
<div class="jumbotron alert alert-info text-center" role="alert">
    <p>还没有学生提交答卷哦，点此<a target="_blank" href="/exam/showqrcode?cid={{cid}}&eid={{eid}}">发放试卷</a>吧</p>
</div>
{{/if}}
