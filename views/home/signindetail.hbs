<ol class="breadcrumb col-md-11 margin-20">
  <li><a href="#user">{{username}}</a></li>
  <li><a href="#course/detail?cid={{cid}}">{{course_name}}</a></li>
  <li class="active">签到结果</li>
</ol>
{{#if signindetail}}
<table class="table table-striped line-btn">
  <thead>
    <tr>
      <th>学号</th>
      <th>姓名</th>
      <th>签到时间</th>
    </tr>
  </thead>
  <tbody>
    {{#each signindetail}}
    <tr onclick="document.location='#course/student?cid={{../cid}}&stu={{stu_id}}'">
      <td>{{stu_id}}</td>
      <td>{{name}}</td>
      <td>{{time}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
{{else}}
<br><br>
<div class="jumbotron alert alert-info text-center" role="alert">
    <p>暂时还没有同学签到，点击<a target="_blank" href="/signin/showqrcode?cid={{cid}}&sid={{sid}}">显示签到二维码</a></p>
</div>
{{/if}}