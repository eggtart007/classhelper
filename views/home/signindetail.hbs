<div class="btn-group" role="group" aria-label="...">
  <button class="btn btn-large btn-default backbtn" type="button" onclick="lc('..')">返回</button>
  <button class="btn btn-large btn-default addCoursebtn" type="button" onclick="lc('.')">刷新</button>
  <button class="btn btn-large btn-default addCoursebtn" type="button" onclick="confirm('确定删除本次签到？本操作不可恢复')&&lc('deletesignin?cid={{course_id}}&sid={{signin_id}}')">删除本次签到</button>
</div>
<table class="table">
  <thead>
    <tr>
      <th>学号</th>
      <th>姓名</th>
      <th>签到时间</th>
    </tr>
  </thead>
  <tbody>
    {{#each signindetail}}
    <tr>
      <td>{{stu_id}}</td>
      <td>{{name}}</td>
      <td>{{time}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
